{
  "name": "@elderjs/plugin-markdown",
  "version": "1.8.0-beta.4",
  "description": "Easily create routes from your markdown files on your Elder.js site using the remark ecosystem.",
  "keywords": [
    "elder.js",
    "elderjs",
    "svelte",
    "ssg",
    "static",
    "markdown",
    "remark"
  ],
  "author": "Nick Reese <npm@nicholasreese.com>",
  "homepage": "https://elderguide.com/tech/elderjs/",
  "license": "MIT",
  "main": "/build/index.js",
  "module": "./build/index.mjs",
  "types": "/build/index.d.ts",
  "exports": {
    "require": "./build/index.js",
    "import": "./build/index.mjs"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elderjs/plugins.git"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build-fast": "tsup src/index.ts --outDir build --format cjs,esm",
    "build": "pnpm run build-fast -- --dts-resolve ",
    "dev": "tsc -p tsconfig.json -w"
  },
  "bugs": {
    "url": "https://github.com/Elderjs/plugins/issues"
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "dependencies": {
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "hast-util-to-string": "^2.0.0",
    "rehype": "^12.0.1",
    "rehype-slug": "^5.0.1",
    "remark": "^14.0.2",
    "remark-extract-frontmatter": "^3.2.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.1",
    "remark-slug": "^6.1.0",
    "shiki": "^0.9.6",
    "unist-util-visit": "^4.1.0",
    "yaml": "1.10.2"
  },
  "devDependencies": {
    "@elderjs/elderjs": "file:../../../elderjs",
    "@types/fs-extra": "^9.0.8",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "tsup": "6.0.1",
    "vitest": "^0.14.2"
  }
}
