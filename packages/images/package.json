{
  "name": "@elderjs/plugin-images",
  "version": "1.8.0-beta.3",
  "description": "Easily generate and manage responsive images for your Elder.js site.",
  "keywords": [
    "elderjs",
    "elder.js",
    "Svelte",
    "responsive",
    "picture",
    "images",
    "lqip",
    "sharp",
    "generator",
    "sapper"
  ],
  "author": "Nick Reese <npm@nicholasreese.com>",
  "homepage": "https://elderguide.com/tech/elderjs/",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "types": "/build/index.d.ts",
  "exports": {
    "require": "./build/index.js",
    "import": "./build/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elderjs/plugins.git"
  },
  "scripts": {
    "build-fast": "tsup src/index.ts --outDir build --format cjs,esm",
    "build": "pnpm run build-fast -- --dts-resolve ",
    "prepublishOnly": "pnpm run build",
    "dev": "tsc -p tsconfig.json -w"
  },
  "bugs": {
    "url": "https://github.com/Elderjs/plugins/issues"
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "peerDependencies": {
    "vanilla-lazyload": "^17.1.2"
  },
  "dependencies": {
    "aws-sdk": "^2.922.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "sharp": "^0.29.0",
    "worker-nodes": "^2.3.1"
  },
  "devDependencies": {
    "@elderjs/elderjs": "file:../../../elderjs",
    "tsup": "6.0.1",
    "typescript": "4.5.4"
  }
}
